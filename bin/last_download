#!/usr/bin/env node

const { exec } = require("child_process");

exec("ls -t  ~/Downloads | head -n 1", (error, stdout, stderr) => {
  if (error) {
    console.log(`error: ${error.message}`);
    return;
  }
  if (stderr) {
    console.log(`stderr: ${stderr}`);
    return;
  }
  console.log(`${stdout.trim()}`);
});
